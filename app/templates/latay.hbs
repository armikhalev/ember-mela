<div class="latayPage">
    <div class="flipContainer" v-on:click="flipCard(card)"> 
        {{#each cards as |card|}}
            <div class="flipper">
                <div class="card frontCard">{{card.front}}</div>
                <div class="card backCard">{{card.back}}</div>
            </div>
        {{/each}}
    </div>
</div>
